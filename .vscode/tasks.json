{
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: g++ build active file",
			"command": "/usr/bin/g++",
			"args": ["-w","main.cpp" ,
			"Server.cpp",
			"Channel.cpp",
			"Client.cpp",
			"PollManager.cpp",
			"tools.cpp",
			"commands.cpp",
			"replies.cpp",
			"mode_command.cpp",
			"-fdiagnostics-color=always",
			"-g",
			"-o",
			"${fileDirname}/${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "Task generated by Debugger."
		}
	],
	"version": "2.0.0"
}